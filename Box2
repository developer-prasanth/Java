class Shape
{
float peri;
void peri(float length,float breadth)
{
this.peri=2*(length+breadth);
}
}
class Rectangle extends Shape
{
void parameters(float length,float breadth,float perimeter)
{
super.peri(length,breadth);
System.out.print("Perimeter of this rectangle is" +super.peri);
}
}
class Box2
{
public static void main(String args[])
{
Rectangle r1=new Rectangle();
r1.parameters(12.5f,102.5f,44f);
}
}
